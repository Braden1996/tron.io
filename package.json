{
  "name": "tron.io",
  "version": "1.0.0",
  "description": "A multiplayer browser implementation of the game tron including an AI computer player.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-build": "npm run client-build:js && npm run client-build:css",
    "client-build:js": "browserify ./src/site/assets/js/entry.js -o ./src/site/static/js/main.js -t [ babelify --presets [ es2015 ] ]",
    "client-build:css": "cleancss ./src/site/assets/css/main.css -o ./src/site/static/css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Braden1996/tron.io.git"
  },
  "author": "Braden Marshall",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Braden1996/tron.io/issues"
  },
  "homepage": "https://github.com/Braden1996/tron.io#readme",
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "clean-css-cli": "^4.0.0"
  }
}
