{
  "name": "tron.io",
  "version": "1.0.0",
  "description": "A multiplayer browser implementation of the game tron including an AI computer player.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "babel-node internal/scripts/build --optimize",
    "build:dev": "babel-node internal/scripts/build",
    "clean": "babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "develop": "babel-node internal/development",
    "lint": "eslint client server shared config internal",
    "lint:fix": "eslint client server shared config internal --fix",
    "preinstall": "node internal/scripts/preinstall",
    "start": "node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Braden1996/tron.io.git"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Braden1996/tron.io/issues"
  },
  "homepage": "https://github.com/Braden1996/tron.io#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "dotenv": "4.0.0",
    "express": "4.15.0",
    "fast-json-patch": "^1.1.8",
    "helmet": "3.4.1",
    "hpp": "0.2.1",
    "immutable": "^3.8.1",
    "modernizr": "3.3.1",
    "normalize.css": "5.0.0",
    "offline-plugin": "4.6.1",
    "react": "15.4.2",
    "react-async-component": "0.2.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-redux": "^5.0.3",
    "react-router-dom": "4.0.0-beta.7",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "serialize-javascript": "1.3.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "socketio-wildcard": "^0.3.0",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.2.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-template": "6.23.0",
    "chokidar": "1.6.1",
    "css-loader": "0.26.2",
    "enzyme": "2.7.1",
    "enzyme-to-json": "1.5.0",
    "eslint": "3.17.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.1",
    "glob": "7.1.1",
    "happypack": "3.0.3",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "md5": "2.2.1",
    "modernizr-loader": "1.0.1",
    "node-notifier": "5.0.2",
    "react-addons-test-utils": "15.4.2",
    "regenerator-runtime": "0.10.3",
    "rimraf": "2.6.1",
    "semver": "5.3.0",
    "source-map-support": "0.4.11",
    "style-loader": "0.13.2",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "2.3.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
